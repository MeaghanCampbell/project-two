  {{#if searchedWorkout}}
    {{#each searchedWorkout as |workout|}}
    <section>
      <div class="section-header">
        <div class="section-header-content">
          <a href="/workout/{{workout.id}}" class="username">{{workout.user.username}}</a>
          <em class="date">{{workout.date}}</em>
        </div>
      </div>
      <a>
        <div class="section-body">
          <p>{{workout.category}} | <em>{{workout.time}} minutes</em> | {{workout.level}}</p>
        </div>
      </a>
    </section>
    {{/each}}
    {{/if}}